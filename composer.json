{
    "name": "dragon-code/web-core",
    "description": "Web core for the Dragon Code apps",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://github.com/andrey-helldar"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/TheDragonCode"
        },
        {
            "type": "open_collective",
            "url": "https://opencollective.com/dragon-code"
        },
        {
            "type": "boosty",
            "url": "https://boosty.to/dragon-code"
        },
        {
            "type": "yoomoney",
            "url": "https://yoomoney.ru/to/410012608840929"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "archtechx/enums": "^0.3",
        "doctrine/dbal": "^3.4",
        "dragon-code/contracts": "^2.19",
        "dragon-code/extended-routes": "^3.1",
        "dragon-code/laravel-cache": "^3.2",
        "dragon-code/laravel-http-logger": "^1.2",
        "dragon-code/laravel-json-response": "^2.3",
        "dragon-code/laravel-migration-actions": "^3.0",
        "dragon-code/laravel-route-names": "^1.3",
        "dragon-code/laravel-support": "^3.4",
        "dragon-code/simple-dto": "^2.5",
        "dragon-code/support": "^6.6",
        "guzzlehttp/guzzle": "^7.5",
        "laravel-lang/attributes": "^2.0",
        "laravel-lang/http-statuses": "^3.0",
        "laravel-lang/lang": "^12.0",
        "laravel-lang/publisher": "^14.3",
        "laravel/framework": "^9.35.1",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "dragon-code/env-sync-laravel": "^2.3",
        "dragon-code/pretty-routes": "^3.2",
        "fakerphp/faker": "^1.20",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.3",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ignition": "^1.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\WebCore\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/*": true,
            "symfony/*": true,
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "friendsofphp/php-cs-fixer": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DragonCode\\WebCore\\ServiceProvider"
            ]
        }
    }
}
